       <import-route-targets>
        <route-targets>
         <route-target>
          <type>as</type>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>7</as>
           <as-index>2222</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
