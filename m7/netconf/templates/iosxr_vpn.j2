<config xmlns:xc="urn:ietf:params:xml:n:netconf:base:1.0" operation="replace">
  <vrfs xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-rsi-cfg">
   <vrf>
    <vrf-name>TEST</vrf-name>
    <create/>
    <description>python automation testing</description>
    <afs>
     <af>
      <af-name>ipv4</af-name>
      <saf-name>unicast</saf-name>
      <topology-name>default</topology-name>
      <create/>
