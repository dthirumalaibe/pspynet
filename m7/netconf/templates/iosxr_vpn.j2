           <as-xx>0</as-xx>
           <as>7</as>
           <as-index>6</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>7</as>
           <as-index>8</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>7</as>
           <as-index>10</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
         </route-target>
        </route-targets>
       </import-route-targets>
      </bgp>
     </af>
    </afs>
   </vrf>
  </vrfs>
</config>
